        "publish": "cd ./dist && npm publish",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ngKit/ngKit.git"
    },
    "author": "Thiery Laverdure",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ngKit/ngKit/issues"
    },
    "homepage": "https://github.com/ngKit/ngKit#readme",
    "dependencies": {
        "cookie-storage": "^3.2.0",
        "faker": "^4.1.0",
        "localforage": "^1.7.3",
        "localforage-cordovasqlitedriver": "^1.7.0",
        "lodash": "^4.17.11",
        "rxjs": "^6.2.0"
    },
    "peerDependencies": {},
    "devDependencies": {
        "@angular/animations": "^7.0.0",
        "@angular/common": "^7.0.0",
        "@angular/compiler": "^7.0.0",
        "@angular/compiler-cli": "^7.2.5",
        "@angular/core": "^7.0.0",
        "@angular/platform-browser": "^7.0.0",
        "@angular/platform-browser-dynamic": "^7.0.0",
        "@angular/platform-server": "^7.0.0",
        "@angular/router": "^7.0.0",
        "@types/faker": "^4.1.5",
        "@types/lodash": "^4.14.116",
        "ng-packagr": "^3.0.6",
        "tsickle": "^0.33.1",
        "typescript": "~3.1.3",
        "zone.js": "^0.8.26"
    }
