{
    "name": "ngkit",
    "version": "1.0.13",
    "description": "",
    "typings": "dist/index.d.ts",
    "$schema": "./node_modules/ng-packagr/package.schema.json",
    "ngPackage": {
        "lib": {
            "entryFile": "src/index.ts"
        },
        "whitelistedNonPeerDependencies": [
            "cookie-storage",
            "faker",
            "localforage",
            "lodash",
            "rxjs"
        ]
